{
	"handshake": {
		"server_bound": [
			{
				"name": "Handshake",
				"id":"0x00",
				"fields": [
					{"name": "version", "type": "varint"},
					{"name": "server_address", "type": "string"},
					{"name": "server_port", "type": "u16"},
					{"name": "next_state", "type": "varint"}
				]
			},
			{
				"name": "LegacyPing",
				"id": "0xFE",
				"fields": [
					{"name": "payload", "type": "u8"}
				]
			}
		]
	},
	
	"status": {
		"client_bound": [
			{
				"id": "0x00",
				"name": "Response",
				"fields": [
					{"name": "json_length", "type": "varint"},
					{"name": "json_response", "type": "string"}
				]
			},
			{
				"id": "0x01",
				"name": "Pong",
				"fields": [
					{"name": "payload", "type": "i64"}
				]
			}
		],
		"server_bound": [
			{
				"name": "Request",
				"id": "0x00",
				"fields":[]
			},
			{
				"name": "Ping",
				"id": "0x01",
				"fields": [
					{"name": "payload", "type": "i64"}
				]
			}
		]
	},
	"login": {
		"client_bound": [
			{
				"name": "Disconnect",
				"id": "0x00",
				"fields":[
					{"name": "reason", "type": "string"}
				]
			},
			{
				"name": "EncryptionRequest",
				"id": "0x01",
				"fields": [
					{"name": "server_id", "type": "string"},
					{"name": "pub_key_len", "type": "varint"},
					{"name": "pub_key", "type": "byte_array(pub_key_len)"},
					{"name": "verify_token_len", "type": "varint"},
					{"name": "verify_token", "type": "byte_array(verify_token_len)"}
				]
			},
			{
				"name":"LoginSuccess",
				"id": "0x02",
				"fields": [
					{"name": "uuid", "type": "string"},
					{"name": "username", "type": "string"}
				]
			},
			{
				"name": "SetCompression",
				"id": "0x03",
				"fields": [
					{"name": "threshold", "type":"varint"}
				]
			},
			{
				"name": "LoginPluginRequest",
				"id": "0x04",
				"fields": [
					{"name": "message_id", "type": "varint"},
					{"name": "channel", "type": "string"},
					{"name": "data", "type": "byte_array"}
				]
			}
		],
		"server_bound": [
			{
				"name": "LoginStart",
				"id": "0x00",
				"fields": [
					{"name": "name", "type": "string"}
				]
			},
			{
				"name": "LoginPluginResponse",
				"id": "0x01",
				"fields": [
					{"name": "shared_secret_len", "type": "varint"},
					{"name": "shared_secret", "type": "byte_array(shared_secret_len)"},
					{"name": "verify_token_len", "type": "varint"},
					{"name": "verify_token", "type": "byte_array(verify_token_len)"}
				]
			},
			{
				"name": "LoginPluginResponse",
				"id": "0x02",
				"fields": [
					{"name": "message_id", "type": "varint"},
					{"name": "successful", "type": "bool"},
					{"name": "data", "type": "byte_array(buffer.remaining())"}
				]
			}
		]
	}
}